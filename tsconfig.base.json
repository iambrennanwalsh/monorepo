{
	"compilerOptions": {
		"composite": true,
		"declaration": true,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"lib": ["esnext", "dom"],
		"module": "esnext",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext"
	}
}
