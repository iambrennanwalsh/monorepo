{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"target": "es5",
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"noEmit": false
	},
	"include": ["next-env.d.ts", "src"],
	"exclude": ["node_modules"],
	"references": [
		{
			"path": "../packages/core"
		}
	]
}
